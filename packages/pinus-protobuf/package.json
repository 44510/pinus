{
  "name": "pinus-protobuf",
  "version": "1.3.11",
  "main": "./dist/protobuf",
  "types": "./lib/protobuf",
  "dependencies": {
    "@types/node": "^8.5.2"
  },
  "files": [
    "dist",
    "lib",
    "test"
  ],
  "scripts": {
    "build": "tsc",
    "start": "yarn run build && node dist/index",
    "lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts",
    "fix-lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts --fix",
    "test": "yarn run build",
    "cov": "nyc mocha",
    "ci": "yarn run test",
    "gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/pinus-protobuf lib/"
  },
  "engines": {
    "node": ">=8.5"
  },
  "devDependencies": {
    "mocha": "6.1.4",
    "should": "13.2.3",
    "tslint": "5.18.0",
    "typescript": "3.5.2"
  }
}
